<h1>Listing tipo_funcs</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_funcs.each do |tipo_func| %>
  <tr>
    <td><%= tipo_func.tipo %></td>
    <td><%= link_to 'Show', tipo_func %></td>
    <td><%= link_to 'Edit', edit_tipo_func_path(tipo_func) %></td>
    <td><%= link_to 'Destroy', tipo_func, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

